{
  "name": "divi-project-pack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "divi-scripts": "1.0.2-rc.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redux": "^4.0.5",
    "video-react": "^0.14.1"
  },
  "scripts": {
    "global:scss": "sass styles/main.scss:styles/css/dpp.css --style compressed --watch",
    "start": "divi-scripts start & npm run global:scss",
    "build": "divi-scripts build",
    "zip": "divi-scripts build && divi-scripts zip",
    "eject": "divi-scripts eject"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "cde": {
    "gettext": "dipp-divi-project-pack",
    "prefix": "dipp"
  }
}
